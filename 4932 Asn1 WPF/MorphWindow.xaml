<Window x:Class="_4932_Asn1_WPF.MorphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4932_Asn1_WPF"
        mc:Ignorable="d"
        Title="MorphWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="morphImage" Canvas.Left="auto" Canvas.Top="auto" Margin="0,21,0,0"/>
        <Button Content="Previous Frame" HorizontalAlignment="Left" Margin="310,0,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button Content="Next Frame" HorizontalAlignment="Left" Margin="405,0,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Button Content="Play" HorizontalAlignment="Left" Margin="46,0,0,0" VerticalAlignment="Top" Click="Button_Click_2"/>

    </Grid>
</Window>
